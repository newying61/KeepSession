{
  "name": "Session Keeper",
  "description": "Keep Session",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "alarms",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_icon" : "icon.png",
    "default_title": "Keep Session",
    "default_popup": "popup.html"
  }
}
